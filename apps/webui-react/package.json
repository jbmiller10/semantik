    "@uwdata/vgplot": "^0.20.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "embedding-atlas": "^0.11.0",
    "lucide-react": "^0.525.0",
    "pdfjs-dist": "^4.6.82",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
