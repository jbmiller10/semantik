{
  "name": "semantik",
  "version": "1.0.0",
  "description": "Semantik: High-performance semantic search engine monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "./scripts/dev.sh",
    "build": "./scripts/build.sh",
    "build:frontend": "cd apps/webui-react && npm run build",
    "dev:frontend": "cd apps/webui-react && npm run dev",
    "test:frontend": "cd apps/webui-react && npm test",
    "clean": "rm -rf apps/*/dist apps/*/node_modules node_modules"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "embedding-atlas": "^0.11.0"
  },
  "overrides": {
    "rollup": "4.52.5",
    "glob": "11.1.0",
    "axios": "1.12.2"
  }
}
